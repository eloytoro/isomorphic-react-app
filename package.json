    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "2.1.0-beta.25",
    "webpack-bundle-analyzer": "^1.5.3",
    "webpack-dev-server": "2.1.0-beta.10",
    "webpack-manifest-plugin": "1.1.0",
    "webpack-md5-hash": "^0.0.5",
    "webpack-node-externals": "^1.5.4",
    "whatwg-fetch": "1.0.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.16.0",
    "camelize": "^1.0.0",
    "classnames": "^2.2.5",
    "history": "3.x",
    "lodash": "^4.17.2",
    "matchmedia-polyfill": "^0.3.0",
    "mobile-detect": "^1.3.4",
    "moment": "^2.15.2",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-intl": "^2.1.5",
    "react-redux": "^4.4.5",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "redux-devtools-extension": "^1.0.0",
    "redux-form": "^6.1.1",
    "redux-saga": "^0.12.0",
    "webfontloader": "^1.6.26"
  },
  "scripts": {
    "start": "NODE_ENV=development gulp start:app",
    "start:test": "NODE_ENV=test gulp start:test",
    "build": "NODE_ENV=production gulp build:app",
    "build:vendor": "NODE_ENV=production gulp build:vendor",
    "build:server": "NODE_ENV=production gulp build:server",
    "storybook": "start-storybook -c ./config/storybook -p 8082 -h 0.0.0.0",
    "test": "jest"
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      "lodash",
      "react-hot-loader/babel"
    ],
    "env": {
      "test": {
        "plugins": [
          "transform-es2015-modules-commonjs"
        ]
      }
    }
  },
  "eslintConfig": {
    "globals": {
      "APP_CONFIG": true,
      "__CLIENT__": true,
      "__SERVER__": true,
      "Promise": true
    },
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "node": true
    },
    "settings": {
      "import/parser": "webpack"
    },
    "plugins": [
      "react"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "rules": {
      "no-console": [
        1
      ]
    }
  },
  "jest": {
    "moduleFileExtensions": [
      "js"
    ],
    "moduleDirectories": [
      "node_modules",
      "bower_components",
      "src"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/config/jest/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    }
  }
}
